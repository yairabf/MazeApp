<Window x:Class="Wpf_Client.view.SinglePlayerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf_Client.view"
        mc:Ignorable="d"
        Title="SinglePlayerWindow" Height="300" Width="300">
    <Grid>
        <local:MazeBoard x:Name="my_mazeBoard" MazeStringUC = "{Binding MazeAsString}" RowsUC="{Binding Rows}" ColsUC="{Binding Cols}"  Loaded="mazeBoard_Loaded"></local:MazeBoard>
    </Grid>
</Window>

